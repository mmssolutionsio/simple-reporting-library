{
  "name": "simple-reporting-library-test-project",
  "label": "skeleton",
  "version": "0.0.1",
  "author": "nsgroup",
  "assets": {
    "css": [
      "./assets/style.css"
    ],
    "js": [
      "./assets/ldd.js"
    ]
  },
  "wrapper": "<div class=\"srl-article-root doc-section\"></div>",
  "imageRatios": {
    "cover": {
      "label": "Cover",
      "ratio": "210x205"
    },
    "a4": {
      "label": "A4",
      "ratio": "70x99"
    },
    "3:2": {
      "label": "3:2",
      "ratio": "3x2"
    },
    "1:1": {
      "label": "1:1",
      "ratio": "1x1"
    },
    "4:3": {
      "label": "4:3",
      "ratio": "4x3"
    },
    "16:9": {
      "label": "16:9",
      "ratio": "16x9"
    }
  },
  "componentProperties": {
    "web-width": {
      "label": "Web width",
      "type": "select",
      "options": [
        {
          "caption": "Text width"
        },
        {
          "caption": "Content width",
          "value": "srl-article__grid--content-web"
        },
        {
          "caption": "Full width",
          "value": "srl-article__grid--full-web"
        }
      ]
    },
    "pdf-width": {
      "label": "PDF width",
      "type": "select",
      "options": [
        {
          "caption": "Text width"
        },
        {
          "caption": "Content width",
          "value": "srl-article__grid--content-pdf"
        },
        {
          "caption": "Full width",
          "value": "srl-article__grid--full-pdf"
        }
      ]
    },
    "show-mobile-image": {
      "label": "Show mobile image",
      "type": "option",
      "value": "show-mobile-image"
    }
  },
  "groups": [
    {
      "label": "Media",
      "components": [
        "image",
        "table"
      ]
    },
    {
      "label": "Text",
      "components": [
        "paragraph"
      ]
    },
    {
      "label": "Titles",
      "components": [
        "title-h1",
        "title-h2",
        "title-h3",
        "title-h4",
        "title-h5",
        "title-h6"
      ]
    }
  ],
  "defaultComponents": {
    "paragraph": "paragraph",
    "image": "image"
  },
  "defaultContent": [
    {
      "component": "title-h1"
    },
    {
      "component": "lead"
    },
    {
      "component": "paragraph"
    }
  ],
  "prefilledComponents": {},
  "metadata": [
    {
      "identifier": "wp_cf_prettyTitle",
      "type": "text",
      "matches": [
        "title-h1.text",
        "title-h2.text",
        "title-h3.text",
        "title-h4.text",
        "title-h5.text",
        "title-h6.text"
      ]
    },
    {
      "identifier": "menuTitle",
      "type": "text",
      "matches": [
        "title-h1.text",
        "title-h2.text",
        "title-h3.text",
        "title-h4.text",
        "title-h5.text",
        "title-h6.text"
      ]
    },
    {
      "identifier": "wp_cf_prettyNumber",
      "type": "text",
      "matches": [
        "title-h2.number",
        "title-h3.number",
        "title-h4.number"
      ]
    },
    {
      "identifier": "notesIdentifier",
      "type": "text",
      "matches": [
        "title-h2.number",
        "title-h3.number",
        "title-h4.number"
      ]
    },
    {
      "identifier": "wp_cf_excerpt",
      "type": "text",
      "matches": [
        "lead.text",
        "paragraph.text"
      ]
    },
    {
      "identifier": "wp_img-cf_featuredImage",
      "type": "image",
      "matches": [
        "image.desktop-image"
      ]
    }
  ]
}