{
  "name": "pdf-toc-page",
  "label": "ToC page",
  "properties": ["pdf-hide-paging-publication-chapter-title", "toc-page-type"],
  "directives": {
    "toc": {
      "allowedChildren": [
        "pdf-toc-item-title",
        "pdf-toc-item",
        "pdf-toc-item-blank"
      ],
      "defaultComponents": {
        "paragraph": "pdf-toc-item"
      },
      "defaultContent": [
        {
          "component": "pdf-toc-item"
        }
      ]
    }
  }
}
